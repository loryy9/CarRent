<div class="container mt-4">
    <div class="d-flex justify-content-between align-items-center mb-4">
        <h3 class="text-white">Elenco dei pacchetti aggiuntivi</h3>
        <a href="/dashboard?alert=inserimentoPacchetto" class="btn btn-warning">
            <i class="fas fa-plus"></i> Aggiungi Pacchetto
        </a>
    </div>
    <div class="row">
        <% if (pacchetti.length > 0) { %>
            <% pacchetti.forEach((pacchetto) => { %>
                <div class="col-md-4 mb-3">
                    <%= pacchetto.nome %>
                </div>
            <% }) %>
        <% } else { %>
            <p class="text-white">Nessuna pacchetto disponibile.</p>
        <% } %>
    </div>
</div>